@model ElektrikDunyasi.Models.Entity.MainBanner
@{
    ViewBag.Title = "MainBannerEkle";
    Layout = "~/Views/Shared/Layout.cshtml";
}
<form style="margin-top:15px" class="form-group container "
      method="post" enctype="multipart/form-data">
    <div class="form-group row ">
        <label class="ekle">Başlık</label>
        <input type="text" class="form-control" name="MainBannerBaslik" />

    </div>


    <div class="form-group row ">
        <label class="ekle">Link Url</label>
        <input type="text" class="form-control" name="LinkUrl" />

    </div>


    <div class="form-group row ">
        <label class="ekle">Tarih</label>
        <input type="datetime-local" class="form-control" name="tarih" />

    </div>
    <div class="form-group row ">
        <label class="ekle">Banner Resim</label>
        <input style="margin-left:20px" type="file" name="file" />

    </div>


    <div style="margin-top:15px;" class="container">
        <button class="btn btn-info" id="MainBannerEkleBtn">Banner Ekle</button>
    </div>



</form>
